<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="match_parent"
	android:layout_width="match_parent"
	android:orientation="vertical">
	<TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
		android:text="PREVIOUS MONTH'S DEDUCTION"
		android:textSize="48px"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previousLifeStyleRelief"
		android:hint="LifeStyle Relief[Up to RM2500]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previousSOCSOContribution"
		android:hint="SOCSO Contribution[Up to RM250]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previouslifeInsurance"
		android:hint="Life Insurance[Up to RM3000]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previousBasicEquipment"
		android:hint="Supporting equipment[Up to RM6000]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previousEducationYourSelf"
		android:hint="Education Fees For Yourself[Up to RM7000]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previousMedicalExamintion"
		android:hint="Complete medical examination[Up to RM500]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previousMedicalDisease"
		android:hint="Medical expenses for serious diseases[Up to RM6000]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previousSSPN"
		android:hint="Net deposit in SSPN[Up to RM8000]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previousPRS"
		android:hint="Private Retirement Scheme Contributions[Up to RM3000]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previousSmallKidEducation"
		android:hint="Fees to childcare centres or kindergartens[Up to RM2000]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previousBreastFeedingEquipment"
		android:hint="Breastfeeding equipment[Up to RM1000]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previousAlimonyFormerWife"
		android:hint="Payment of alimony to former wife[Up to RM4000]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previousEMInsurance"
		android:hint="Education and medical insurance premiums[Up to RM3000]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previousFatherRelief"
		android:hint="Father Relief[Up to RM1500]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previousMotherRelief"
		android:hint="Mother Relief[Up to RM1500]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<EditText
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/previousMapaRelief"
		android:hint="Medical expenses for own parents[Up to RM5000]"
		android:textSize="34px"
		android:inputType="numberDecimal"
        android:maxLength="20"/>
	<TextView
        android:layout_width="match_parent"
        android:layout_height="1px"
		android:layout_weight="1"/>
	<Button
		android:id="@+id/continuePayroll7"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text="Continue"
        android:textSize="23dp"
        android:backgroundTint="@android:color/holo_green_dark"
        android:textColor="@android:color/white"/>
</LinearLayout>